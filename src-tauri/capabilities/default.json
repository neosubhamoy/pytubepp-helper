{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "main-capability",
  "description": "default permissions",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:default",
    "os:default",
    "fs:default",
    "process:default",
    "notification:default",
    "updater:default",
    "core:window:allow-hide",
    "fs:allow-app-write",
    "fs:allow-app-write-recursive"
  ]
}